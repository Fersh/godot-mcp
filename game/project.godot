; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Save Some Princess"
run/main_scene="res://scenes/main_menu.tscn"
config/features=PackedStringArray("4.5", "Mobile")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/use_filter=false
boot_splash/image="res://splash.png"
config/icon="res://icon1024.png"
boot_splash/minimum_display_time=1500

[autoload]

GameSettings="*res://scripts/game_settings.gd"
AbilityManager="*res://scripts/abilities/ability_manager.gd"
JuiceManager="*res://scripts/juice_manager.gd"
StatsManager="*res://scripts/stats_manager.gd"
PermanentUpgrades="*res://scripts/permanent_upgrades.gd"
CharacterManager="*res://scripts/character_manager.gd"
SoundManager="*res://scripts/sound_manager.gd"
EquipmentManager="*res://scripts/equipment/equipment_manager.gd"
ActiveAbilityManager="*res://scripts/active_abilities/active_ability_manager.gd"
HapticManager="*res://scripts/haptic_manager.gd"
UltimateAbilityManager="*res://scripts/ultimate_abilities/ultimate_ability_manager.gd"
KillStreakManager="*res://scripts/kill_streak_manager.gd"
BloodSplatter="*res://scenes/effects/blood_screen_splatter.tscn"
DifficultyManager="*res://scripts/difficulty_manager.gd"
PrincessManager="*res://scripts/princess_manager.gd"
CurseEffects="*res://scripts/curse_effects_handler.gd"
UnlocksManager="*res://scripts/unlocks_manager.gd"
MissionsManager="*res://scripts/missions_manager.gd"
DailyLoginManager="*res://scripts/daily_login_manager.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=752
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/handheld/orientation=4

[input_devices]

pointing/emulate_touch_from_mouse=true

[rendering]

renderer/rendering_method="mobile"
textures/vram_compression/import_etc2_astc=true
viewport/hdr_2d=true
